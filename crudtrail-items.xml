<?xml version="1.0" encoding="ISO-8859-1"?>

<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	<enumtypes>
		<enumtype code="DiagramProductStatus" autocreate="true" generate="true" dynamic="false">
			<value code="instock" />
			<value code="outofstock" />
		</enumtype>
	</enumtypes>

	<itemtypes>

		<itemtype code="DiagramProduct">
			<deployment table="DiagramProduct" typecode="22123" />
			<attributes>

				<attribute type="java.lang.String" qualifier="code">
					<persistence type="property" />
					<modifiers unique="true" optional="false" initial="true"/>
				</attribute>

				<attribute type="java.lang.String" qualifier="name">
					<persistence type="property" />
				</attribute>

				<attribute type="java.lang.Double" qualifier="price">
					<persistence type="property" />
				</attribute>

				<attribute type="java.lang.String" qualifier="description">
					<persistence type="property" />
				</attribute>

				<attribute type="java.lang.String" qualifier="descriptionShort">
					<persistence type="property" />
				</attribute>

				<attribute type="DiagramProductStatus" qualifier="productStatus">
					<persistence type="property" />
					<defaultvalue>em().getEnumerationValue("DiagramProductStatus","instock")</defaultvalue>
				</attribute>

				<attribute qualifier="productImage" type="MediaContainer">
					<persistence type="property" />
				</attribute>

			</attributes>
		</itemtype>

	</itemtypes>
</items>
